{% form_theme form 'bootstrap_5_layout.html.twig' %}
{{ form_start(form) }}
        <h5>{{ form_label(form, 'recherche')}}</h5>
        {{ form_widget(form.recherche)}}
        <h5>{{ form_label(form, 'country')}}</h5>
        {{ form_widget(form.country , {'attr': {'class':'form-control mb-2'}}) }}
        <h5>{{ form_label(form, 'status')}}</h5>
        {{ form_widget(form.status)}}
        <hr class="my-2">
        <div class="container text-center"><button class="btn btn-primary">Chercher</button></div>
        <hr class="my-2">
{{ form_end(form) }}